<%= form_for(@user) do |f| %>

  <%= f.label :email %>
  <%= f.text_field :email %>
  
  <br>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  
  <br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  
  <br>
  <%= f.label :account_type %>
  <%= f.text_field :account_type %>

  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br>
  <%= f.submit %>

<% end %>
